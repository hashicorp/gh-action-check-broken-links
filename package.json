{
  "name": "@hashicorp/gh-action-check-broken-links",
  "version": "0.0.0",
  "description": "A GitHub Action that checks for the presence of broken links",
  "main": "lib/main.js",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run pack && npm test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "next-hashicorp precommit"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "link",
    "check"
  ],
  "author": "Kevin Pruett",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@hashicorp/nextjs-scripts": "^7.0.0",
    "rehype-parse": "^6.0.2",
    "remark-parse": "^8.0.2",
    "unified": "^9.0.0",
    "unist-util-find": "^1.0.1",
    "unist-util-visit": "^2.0.2"
  },
  "devDependencies": {
    "@octokit/types": "^2.16.2",
    "@types/glob": "^7.1.1",
    "@types/hast-format": "^2.3.0",
    "@types/jest": "^24.9.1",
    "@types/mdast": "^3.0.3",
    "@types/node": "^12.7.12",
    "@types/node-fetch": "^2.5.7",
    "@types/unist": "^2.0.3",
    "@zeit/ncc": "^0.20.5",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.9.2"
  }
}
